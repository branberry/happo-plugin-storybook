{
  "name": "happo-plugin-storybook",
  "version": "3.1.0",
  "description": "A happo.io plugin for projects using Storybook",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/",
    "test": "jest",
    "happo": "happo",
    "build": "babel ./src --out-dir ./",
    "prepublish": "yarn build",
    "storybook": "start-storybook -p 9001 -c .storybook -s public",
    "build-storybook": "build-storybook -c .storybook -o .happo-out -s public"
  },
  "jest": {
    "testMatch": [
      "**/*-test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enduire/happo-plugin-storybook.git"
  },
  "keywords": [
    "screenshot",
    "testing",
    "storybook",
    "happo",
    "visual",
    "regression",
    "testing"
  ],
  "author": "Henric Trotzig <henric.trotzig@happo.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enduire/happo-plugin-storybook/issues"
  },
  "homepage": "https://github.com/enduire/happo-plugin-storybook#readme",
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.0",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.5",
    "@storybook/addons": "^6.5.5",
    "@storybook/api": "^6.5.15",
    "@storybook/core": "^6.5.5",
    "@storybook/react": "^6.5.5",
    "@storybook/testing-library": "^0.0.11",
    "babel-loader": "^8.0.5",
    "eslint": "^8.6.0",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^3.0.1",
    "happo.io": "^5.1.3",
    "raw-loader": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-tether": "^2.0.0",
    "styled-components": "^4.1.3",
    "webpack": "^4.0.0"
  },
  "dependencies": {
    "@babel/runtime": ">=7.0.0",
    "archiver": "^3.0.0",
    "rimraf": "^2.6.3"
  }
}
